{
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "h",
                "h"
            ],
            "after": [
                "<ESC>"
            ]
        },
        {
            "before": [
                "<leader>",
                "."
            ],
            "commands": [
                {
                    "command": "editor.action.triggerSuggest"
                }
            ]
        },
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateBack"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateForward"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                {
                    "command": "extension.runIt"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                {
                    "command": "workbench.action.files.save"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "<CR>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.quickFix",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.quickOpen",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "-"
            ],
            "after": [
                "/"
            ]
        },
        {
            "before": [
                "_"
            ],
            "after": [
                "N"
            ]
        },
        {
            "before": [
                "B"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "E"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "c"
            ],
            "after": [
                "<UP>"
            ]
        },
        {
            "before": [
                "h"
            ],
            "after": [
                "<LEFT>"
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "<RIGHT>"
            ]
        },
        {
            "before": [
                "t"
            ],
            "after": [
                "<DOWN>"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "'"
            ],
            "commands": [
                {
                    "command": "extension.embraceSingleQuotes"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "\""
            ],
            "commands": [
                {
                    "command": "extension.embraceDoubleQuotes"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "<"
            ],
            "commands": [
                {
                    "command": "extension.embraceAngleBrackets"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "{"
            ],
            "commands": [
                {
                    "command": "extension.embraceCurlyBrackets"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "("
            ],
            "commands": [
                {
                    "command": "extension.embraceParenthesis"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "["
            ],
            "commands": [
                {
                    "command": "extension.embraceSquareBrackets"
                }
            ]
        },
        {
            "before": [
                "B"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "E"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "c"
            ],
            "after": [
                "<UP>"
            ]
        },
        {
            "before": [
                "h"
            ],
            "after": [
                "<LEFT>"
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "<RIGHT>"
            ]
        },
        {
            "before": [
                "t"
            ],
            "after": [
                "<DOWN>"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<CR>"
            ],
            "after": [
                "n"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "<C-y>"
            ],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        }
    ],
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": false,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-c>": false,
        "<C-p>": false,
    },
    "diffEditor.ignoreTrimWhitespace": true,
    "window.zoomLevel": -1,
    "gitlens.keymap": "alternate",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "editor.formatOnSave": true,
    "typescript.tsdk": "C:\\Users\\dimitar\\AppData\\Roaming\\npm\\node_modules\\typescript\\lib",
    "workbench.activityBar.visible": true,
    "editor.minimap.enabled": false,
    "html.format.wrapAttributes": "force-expand-multiline",
    "gitlens.historyExplorer.enabled": true,
    "editor.wordWrap": "wordWrapColumn",
    "prettier.printWidth": 120,
    "editor.parameterHints": false,
    "editor.wordWrapColumn": 120,
    "macros": {
        "runCurrentTest": [
            "editor.action",
            "cursorUp",
            "editor.action.addCommentLine",
            "cursorDown"
        ]
    }
}